<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_top"
        android:layout_width="match_parent"
        android:layout_height="60dp">

        <ImageView
            android:id="@+id/img_logo_admin"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:src="@drawable/logo_login"
            app:layout_constraintBottom_toBottomOf="@id/constraint_top"
            app:layout_constraintLeft_toLeftOf="@id/constraint_top"
            app:layout_constraintTop_toTopOf="@id/constraint_top" />

        <TextView
            android:id="@+id/tv_name_congty_admin"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="TRANG NHÂN VIÊN "
            android:textColor="#000000"
            android:textSize="17sp"
            app:layout_constraintBottom_toBottomOf="@id/constraint_top"
            app:layout_constraintLeft_toRightOf="@id/img_logo_admin"
            app:layout_constraintTop_toTopOf="@id/constraint_top" />

        <TextView
            android:id="@+id/tv_name_user_login"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical"
            android:text="Chu Văn Quyết"
            android:textColor="#4d4b4b"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@id/constraint_top"
            app:layout_constraintRight_toRightOf="@id/constraint_top"
            app:layout_constraintTop_toTopOf="@id/constraint_top"

            />


    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_zero"
        app:layout_constraintTop_toBottomOf="@id/constraint_top"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/constraint_first"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tv_taikhoansinhvien"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TÀI KHOẢN NHÂN VIÊN"
            android:textColor="#837f7f"
            android:textSize="16sp"/>
        
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
            android:id="@+id/constraint_first"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/constraint_zero"
        android:layout_marginLeft="10dp"
            >

            <TextView
                android:id="@+id/tv_trangchu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Trang chủ"
                android:textSize="20sp"
                app:layout_constraintTop_toTopOf="@id/constraint_first"
                app:layout_constraintLeft_toLeftOf="@id/constraint_first"/>
            <ImageView
                android:id="@+id/img_icon_first"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_arrow_drop_down_black_24dp"
                app:layout_constraintRight_toRightOf="@id/constraint_first"
                app:layout_constraintLeft_toRightOf="@id/tv_trangchu"
                app:layout_constraintTop_toTopOf="@id/constraint_first"
                app:layout_constraintTop_toBottomOf="@id/constraint_first"
                android:layout_marginLeft="115dp"/>



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_second"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_first"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_tienich"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tiện ích"
            android:textSize="20sp"

            app:layout_constraintTop_toTopOf="@id/constraint_second"
            app:layout_constraintLeft_toLeftOf="@id/constraint_second"/>
        <ImageView
            android:id="@+id/img_icon_second"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_second"
            app:layout_constraintLeft_toRightOf="@id/tv_tienich"
            app:layout_constraintTop_toTopOf="@id/constraint_second"
            app:layout_constraintTop_toBottomOf="@id/constraint_second"
            android:layout_marginLeft="135dp"/>



    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_third"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_second"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_chiasethongtin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Chia sẻ thông tin"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@id/constraint_third"
            app:layout_constraintLeft_toLeftOf="@id/constraint_third"/>
        <ImageView
            android:id="@+id/img_icon_third"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_third"
            app:layout_constraintLeft_toRightOf="@id/tv_chiasethongtin"
            app:layout_constraintTop_toTopOf="@id/constraint_third"
            app:layout_constraintTop_toBottomOf="@id/constraint_third"
            android:layout_marginLeft="55dp"/>



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_third"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_thongtincanhan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Thông tin cá nhân"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@id/constraint_four"
            app:layout_constraintLeft_toLeftOf="@id/constraint_four"/>
        <ImageView
            android:id="@+id/img_icon_four"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_four"
            app:layout_constraintLeft_toRightOf="@id/tv_thongtincanhan"
            android:layout_marginLeft="45dp"/>



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_five"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_four"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_theodoigiaodich"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Theo dõi giao dịch"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="@id/constraint_five"
            app:layout_constraintLeft_toLeftOf="@id/constraint_five"/>
        <ImageView
            android:id="@+id/img_icon_five"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_five"
            app:layout_constraintLeft_toRightOf="@id/tv_theodoigiaodich"
            android:layout_marginLeft="40dp"
            />



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_six"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_five"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_chuongtrinhdaotao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="Chương trình đào tạo"
            app:layout_constraintTop_toTopOf="@id/constraint_six"
            app:layout_constraintLeft_toLeftOf="@id/constraint_six"/>
        <ImageView
            android:id="@+id/img_icon_six"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_six"
            app:layout_constraintLeft_toRightOf="@id/tv_chuongtrinhdaotao"
            android:layout_marginLeft="15dp"
            />



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_seven"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_six"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_lichlam"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="Lịch làm việc"
            app:layout_constraintTop_toTopOf="@id/constraint_seven"
            app:layout_constraintLeft_toLeftOf="@id/constraint_seven"/>
        <ImageView
            android:id="@+id/img_icon_seven"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_seven"
            app:layout_constraintLeft_toRightOf="@id/tv_lichlam"
            android:layout_marginLeft="88dp"
            />



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_eight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_seven"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_bangluong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="Bảng lương"
            app:layout_constraintTop_toTopOf="@id/constraint_eight"
            app:layout_constraintLeft_toLeftOf="@id/constraint_eight"/>
        <ImageView
            android:id="@+id/img_icon_eight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_eight"
            app:layout_constraintLeft_toRightOf="@id/tv_bangluong"
            android:layout_marginLeft="103dp"
            />



    </android.support.constraint.ConstraintLayout>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_nine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_weight="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/constraint_eight"
        android:layout_marginLeft="10dp"
        >

        <TextView
            android:id="@+id/tv_danhgiacongviev"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="Đánh giá công việc"
            app:layout_constraintTop_toTopOf="@id/constraint_nine"
            app:layout_constraintLeft_toLeftOf="@id/constraint_nine"/>
        <ImageView
            android:id="@+id/img_icon_nine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_drop_down_black_24dp"
            app:layout_constraintRight_toRightOf="@id/constraint_nine"
            app:layout_constraintLeft_toRightOf="@id/tv_danhgiacongviev"
            android:layout_marginLeft="36dp"
            />



    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraint_right"
        android:layout_width="1044dp"
        android:layout_height="497dp"
        app:layout_constraintLeft_toRightOf="@id/constraint_first"
        app:layout_constraintTop_toBottomOf="@id/constraint_top">

        <FrameLayout
            android:id="@+id/framelayout_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>